<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>CreateTeamServlet</servlet-name>
        <servlet-class>Turnieje.Servlets.AACreateTeamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageTeamServlet</servlet-name>
        <servlet-class>Turnieje.Servlets.AAManageTeamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTournamentServlet</servlet-name>
        <servlet-class>Turnieje.Servlets.AACreateTournamentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageTournamentServlet</servlet-name>
        <servlet-class>Turnieje.Servlets.AAManageTournamentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareCreateTeamServlet</servlet-name>
        <servlet-class>Turnieje.Servlets.AAPrepareCreateTeamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareCreateTournamentServlet</servlet-name>
        <servlet-class>Turnieje.Servlets.AAPrepareCreateTournamentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareManageTeam</servlet-name>
        <servlet-class>Turnieje.Servlets.AAPrepareManageTeam</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareManageTournament</servlet-name>
        <servlet-class>Turnieje.Servlets.AAPrepareManageTournament</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareTeamsList</servlet-name>
        <servlet-class>Turnieje.Servlets.AAPrepareTeamsList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareTournamentsList</servlet-name>
        <servlet-class>Turnieje.Servlets.AAPrepareTournamentsList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareMatchesList</servlet-name>
        <servlet-class>Turnieje.Servlets.AAPrepareMatchesListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnterResult</servlet-name>
        <servlet-class>Turnieje.Servlets.AAEnterResultServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationActivate</servlet-name>
        <servlet-class>Turnieje.Servlets.RegistrationActivate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PasswordChange</servlet-name>
        <servlet-class>Turnieje.Servlets.PasswordChange</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountFinalyDeleted</servlet-name>
        <servlet-class>Turnieje.Servlets.AccountFinalyDeleted</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AALogout</servlet-name>
        <servlet-class>Turnieje.Servlets.AALogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrepareSendEmail</servlet-name>
        <servlet-class>Turnieje.Servlets.PrepareSendEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateEmail</servlet-name>
        <servlet-class>Turnieje.Servlets.CreateEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateTeamServlet</servlet-name>
        <url-pattern>/CreateTeam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageTeamServlet</servlet-name>
        <url-pattern>/ManageTeam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateTournamentServlet</servlet-name>
        <url-pattern>/CreateTournament</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageTournamentServlet</servlet-name>
        <url-pattern>/ManageTournament</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareCreateTeamServlet</servlet-name>
        <url-pattern>/PrepareCreateTeamServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareCreateTournamentServlet</servlet-name>
        <url-pattern>/PrepareCreateTournamentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareManageTeam</servlet-name>
        <url-pattern>/PrepareManageTeam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareManageTournament</servlet-name>
        <url-pattern>/PrepareManageTournament</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareTeamsList</servlet-name>
        <url-pattern>/PrepareTeamsList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareTournamentsList</servlet-name>
        <url-pattern>/PrepareTournamentsList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareMatchesList</servlet-name>
        <url-pattern>/PrepareMatchesList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnterResult</servlet-name>
        <url-pattern>/EnterResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationActivate</servlet-name>
        <url-pattern>/RegistrationActivate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PasswordChange</servlet-name>
        <url-pattern>/PasswordChange</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountFinalyDeleted</servlet-name>
        <url-pattern>/AccountFinalyDeleted</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AALogout</servlet-name>
        <url-pattern>/AALogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrepareSendEmail</servlet-name>
        <url-pattern>/PrepareSendEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateEmail</servlet-name>
        <url-pattern>/CreateEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

</web-app>
